.assign-container {
  width: 100%;
  height: 100%;
  background-image: url(../../../image/background.png);
  background-size: cover;
  position: relative;
}
.assign-title{
  font-size: 24px;
  text-align: center;
  padding-top: 40px;
  padding-bottom: 40px;
}
.assign-image-area {
  background-image: url(../../../image/photoframe.png);
  background-size: cover;
  width: 250px;
  height: 260px;
  margin: 0 auto;
  text-align:center;
  position: relative;
}
.assign-image {
  width: 220px;
  height: 220px;
  margin-top: 30px;
  border-radius: 10px;
}
.assign-number {
  position: absolute;
  width: 60px;
  height: 60px;
  background-color: rgb(14,14,38);
  border-radius: 10px;
  left: 10px;
  top: 25px;
  color: white;
  line-height: 60px;
  font-size: 30px;
}
.assign-character {
  background-image: url(../../../image/characterFrame.png);
  background-size: cover;
  width: 250px;
  height: 52px;
  text-align: center;
  margin: 20px auto;
  line-height: 50px;
  font-size: 30px;
}
.check-identify {
  background-image: url(../../../image/Button/check_identity.png);
  background-size: cover;
  width: 250px;
  height: 60px;
  margin: 50px auto;
}
.next-one {
  background-image: url(../../../image/Button/next_one.png);
  background-size: cover;
  width: 250px;
  height: 60px;
  margin: 50px auto;
}
.return-judge {
  background-image: url(../../../image/Button/return_judge.png);
  background-size: cover;
  width: 250px;
  height: 60px;
  margin: 50px auto;
}

.popup-tips {
  width: 260px;
  height: 260px;
  background-color: rgba(255,255,255,.9);
  position: absolute;
  bottom: 80px;
  left: 50%;
  margin-left: -130px;
  border-radius: 10px;
  text-align: center;
}
.popup-tips-image {
  width: 200px;
  height: 200px;
  margin-top: 15px;
  margin-bottom: 10px;
}